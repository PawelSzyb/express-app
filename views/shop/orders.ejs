<%- include("../partials/header.ejs") %> <%- include("../partials/navbar.ejs")
%>
<main>
  <% if(orders.length <= 0 ) {%>
  <h2>Nothing there...</h2>
  <% } else { %>
  <ul>
    <% orders.forEach(order => { %>
    <li>
      <h3># <%= order._id %></h3>
      <ul>
        <% order.items.forEach(product => { %>
        <li><%= product.title %> - <%= product.quantity %></li>
        <% }) %>
      </ul>
    </li>
    <% }) %>
  </ul>
  <% } %>
</main>
%> <%- include("../partials/footer.ejs") %>
